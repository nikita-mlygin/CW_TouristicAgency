<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:clinet="clr-namespace:NCourseWork.Clients.Open"
                    xmlns:status="clr-namespace:NCourseWork.Status.Open"
                    xmlns:country="clr-namespace:NCourseWork.Country.Open"
                    xmlns:hotel="clr-namespace:NCourseWork.Hotel.Open"
                    xmlns:tour="clr-namespace:NCourseWork.Tour.Open"
                    xmlns:purchase="clr-namespace:NCourseWork.Purchase.Open"
                    >
    <DataTemplate DataType="{x:Type clinet:ClientViewModel}">
        <clinet:ClientView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type status:OpenStatusViewModel}">
        <status:OpenStatusView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type country:OpenCountryViewModel}">
        <country:CountryView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type hotel:HotelViewModel}">
        <hotel:HotelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type tour:OpenTourViewModel}">
        <tour:TourView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type purchase:PurchaseViewModel}">
        <purchase:PurchaseView />
    </DataTemplate>
</ResourceDictionary>